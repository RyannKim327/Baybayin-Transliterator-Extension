{
	"name": "Baybayin Transliterator",
	"version": "0.0.1",
	"author": "RyannKim327",
	"description": "A baybayin transliterator extension for chrome and other browsers.",
	"permissions": [
		"activeTab",
		"clipboardWrite"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": true
	},
	"browser_action": {
		"default_icon": {
		"16": "logo/16.png",
		"32": "logo/32.png",
		"48": "logo/48.png",
		"64": "logo/64.png",
		"128": "logo/128.png"
		}
	  },
	  "icons": {
		"16": "logo/16.png",
		"32": "logo/32.png",
		"48": "logo/48.png",
		"64": "logo/64.png",
		"128": "logo/128.png"
	},
	"manifest_version": 2
}